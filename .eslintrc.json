{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "max-len": ["off"],
        "react/no-array-index-key": ["off"],
        "react/jsx-filename-extension": [1, { "extensions": [".jsx", ".js"] }],
        "import/extensions": ["error", "always", {
            "js": "never",
            "jsx": "never"
        }],
    }
}
